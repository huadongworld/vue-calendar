{
  "name": "@toast-ui/vue-calendar",
  "description": "TOAST UI Calendar for Vue",
  "author": "NHN Cloud FE Development Lab <dl_javascript@nhn.com>",
  "version": "2.1.3",
  "main": "./dist/toastui-vue-calendar.js",
  "types": "./index.d.ts",
  "module": "./dist/toastui-vue-calendar.mjs",
  "exports": {
    ".": {
      "import": "./dist/toastui-vue-calendar.mjs",
      "require": "./dist/toastui-vue-calendar.js"
    },
    "./ie11": "./dist/toastui-vue-calendar.ie11.js",
    "./esm": "./dist/toastui-vue-calendar.mjs"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nhn/tui.calendar.git",
    "directory": "apps/vue-calendar"
  },
  "keywords": [
    "nhn",
    "toast",
    "toastui",
    "toast-ui",
    "calendar",
    "fullcalendar",
    "daily",
    "weekly",
    "monthly",
    "business week",
    "milestone",
    "task",
    "allday"
  ],
  "scripts": {
    "lint": "eslint .",
    "develop": "vite",
    "webpack-develop": "cross-env NODE_ENV=serve webpack-dev-server --mode development --config ./config/webpack.config.js",
    "build": "rimraf dist/ && concurrently 'npm:build:*'",
    "build:modern": "vite build && vite build --mode minify",
    "build:ie11": "vite build --mode ie11 && vite build --mode ie11_minify",
    "build:esm": "vite build --mode esm"
  },
  "license": "MIT",
  "dependencies": {
    "@toast-ui/calendar": "^2.1.3"
  },
  "devDependencies": {
    "@lark-opdev/block-bitable-webpack-utils": "^0.1.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/webpack": "^5.28.0",
    "@vue/cli": "^5.0.6",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "esbuild-loader": "^2.20.0",
    "eslint-plugin-vue": "^9.1.1",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "react-refresh": "^0.14.0",
    "simple-progress-webpack-plugin": "^2.0.0",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.6.4",
    "vite": "^4.5.0",
    "vite-plugin-vue2": "^2.0.2",
    "vue": "^2.6.14",
    "vue-loader": "^17.3.1",
    "vue-template-compiler": "^2.7.15",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0",
    "webpackbar": "^5.0.2"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  }
}
